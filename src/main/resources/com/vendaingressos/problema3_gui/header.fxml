<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vendaingressos.problema3_gui.controllers.ControllerTopBar">
    <children>
        <ToolBar HBox.hgrow="ALWAYS">
            <items>
                <Button mnemonicParsing="false" text="Button" />
            </items>
        </ToolBar>
        <ToolBar>
            <items><MenuButton fx:id="perfil" mnemonicParsing="false" text="MenuButton">
                <items>
                    <MenuItem fx:id="perfilMenu" mnemonicParsing="false" onAction="#irParaPerfil" text="Action 1" />
                    <MenuItem fx:id="logout" mnemonicParsing="false" onAction="#voltarParaLogin" text="Action 2" />
                </items>
            </MenuButton>
            </items>
        </ToolBar>
    </children>
</HBox>
